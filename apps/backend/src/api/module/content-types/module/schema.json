{
  "kind": "collectionType",
  "collectionName": "modules",
  "info": {
    "singularName": "module",
    "pluralName": "modules",
    "displayName": "Module",
    "description": "Modules available for each app"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "app": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::app.app",
      "inversedBy": "modules"
    },
    "pricePerUser": {
      "type": "decimal",
      "default": 0
    },
    "isCore": {
      "type": "boolean",
      "default": false
    },
    "features": {
      "type": "json",
      "default": []
    },
    "icon": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
