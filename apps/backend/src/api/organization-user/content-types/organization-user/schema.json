{
  "kind": "collectionType",
  "collectionName": "organization_users",
  "info": {
    "singularName": "organization-user",
    "pluralName": "organization-users",
    "displayName": "Organization User",
    "description": "Users belonging to organizations"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "organization": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization",
      "inversedBy": "organizationUsers"
    },
    "role": {
      "type": "string",
      "default": "User"
    },
    "customPermissions": {
      "type": "json",
      "default": {}
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "joinedAt": {
      "type": "datetime"
    },
    "lastAccessAt": {
      "type": "datetime"
    }
  }
}
